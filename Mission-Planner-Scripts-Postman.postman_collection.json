					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Send a GET request to {{ base_url }}/queue",
									"pm.sendRequest({",
									"  url: `${pm.environment.get('base_url')}/queue`,",
									"  method: 'GET'",
									"}, function (error, response) {",
									"  if (error) {",
									"    console.error('Error:', error);",
									"    return;",
									"  }",
									"",
									"  // Check if the response is an empty list (an array with no elements)",
									"  pm.test('Response is an empty list', function () {",
									"    pm.expect(response.json()).to.be.an('array').that.is.empty;",
									"  });",
									"});",
